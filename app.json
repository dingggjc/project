{
  "expo": {
    "name": "Tita App",
    "slug": "Tita-App",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/Tita.jpg",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "web": {
      "bundler": "metro",
      "output": "single",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-font",
      "expo-web-browser"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "c64712f6-c9a4-4320-8ba5-a3c5e7095018"
      }
    }
  }
}
